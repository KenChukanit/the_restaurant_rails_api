



<h1>Sign in</h1>
<%= form_with url: session_path, local: true do |f|  %>
<div>
<%= f.label :email %>
<%= f.email_field :email%>
</div>
<div>
    <%= f.label :password  %> 
    <%= f.password_field :password  %> 
</div>
<%= f.submit "Sign In"  %> 
<%= link_to "Sign Up", new_user_path %>
<%= link_to "Anonymous User", foods_path%>
<% end %> 